        <!-- 4) TM-Nummer -->
        <idno type="TM"></idno>
    </msIdentifier>
    <msContents>
        <!-- 15) Inschriftengattung in @class -->
        <msItem class="https://www.eagle-network.eu/voc/typeins/lod/51">
